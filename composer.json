{
    "name": "imemento/http",
    "description": "iMemento HTTP Helpers",
    "homepage": "https://gitlab.com/imemento/composer/packages/http",
    "license": "MIT",
    "authors": [
        {
            "name": "George Stan",
            "email": "contact@georgestan.com",
            "homepage": "http://imemento.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "fideloper/proxy": "^4.2",
        "guzzlehttp/guzzle": "^6.3 || ^7.0",
        "illuminate/http": "^5.0 || ^6.0 || ^7.0",
        "imemento/jwt": "^0.1.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "^7.0 || ^8.0 || ^9.0"
    },
    "autoload": {
        "psr-4": {
            "iMemento\\Http\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "iMemento\\Http\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
