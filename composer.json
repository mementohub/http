{
    "name": "imemento/http",
    "description": "iMemento HTTP",
    "homepage": "https://gitlab.com/imemento/composer/http",
    "license": "MIT",
    "authors": [
        {
            "name": "George Stan",
            "email": "contact@georgestan.com",
            "homepage": "http://imemento.com"
        }
    ],
    "require": {
        "php" : "^5.6|^7.0",
        "imemento/jwt": "dev-master",
        "guzzlehttp/guzzle": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "^5.7.5"
    },
    "autoload": {
        "psr-4": {
            "iMemento\\Http\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "iMemento\\Http\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
